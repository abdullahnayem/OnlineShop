@model int
 
<td style="width: 150px">
    <div class="btn-group">
        <a asp-action="Edit" asp-controller="NewProducts" class="btn btn-primary" asp-route-id="@Model"><i class="fa fa-edit"></i></a>
        <a asp-action="Details" asp-controller="NewProducts" class="btn btn-success" asp-route-id="@Model"><i class="fa fa-list"></i></a>
        @*edit asp-action e may be change hobe*@
        <a asp-action="Delete" asp-controller="NewProducts" class="btn btn-danger" asp-route-id="@Model"><i class="fa fa-trash"></i></a>

    </div>
    
</td>